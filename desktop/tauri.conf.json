{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Redis Dash",
  "version": "0.0.1",
  "identifier": "me.xuco.redis-dash",
  "build": {
    "frontendDist": "../app/dist",
    "devUrl": "http://localhost:5090"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Redis Dash",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.png", "icons/icon.icns", "icons/icon.ico"]
  }
}
